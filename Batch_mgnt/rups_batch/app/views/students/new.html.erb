<h1>Register</h1>
<div>
<%= form_for @students do |f| %>
  <p>
    <%= f.label :first_name %>:
    <%= f.text_field :first_name %>
  </p>
  <p>
    <%= f.label :last_name %>:
    <%= f.text_field :last_name %>
  </p>
 <p>
    <%= f.label :middle_name %>:
    <%= f.text_field :middle_name %>
  </p>

  <p>
    <%= f.label :email, "E-Mail" %>:
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.label :mobile %>:
    <%= f.text_field :mobile %>
  </p>
  <p>
    <%= f.label :state %>:
    <%= f.text_field :state  %>
  </p>
  <p>
    <%= f.label :city %>:
    <%= f.text_field :city %>
  </p>
  <p>
    <%= f.label :zip_code %>:
    <%= f.text_field :zip_code %>
  </p>
  <p>
    <%= f.label :grade %>:
    <%= f.text_field :grade %>
  </p>
  <p>
    <%= f.label :enrollment_date %>:
    <%= f.text_field :enrollment_date %>
  </p>
  <p>
    <%= f.submit "Register" %>
  </p>

<% end %>

</div> 
<%= link_to 'Back', students_path %>
