<h1>Register</h1>
<table>
<div>
<%= form_for @students do |f| %>
   <%= f.label :first_name %>:
    <%= f.text_field :first_name %>
   <%= f.label :last_name %>:
    <%= f.text_field :last_name %>
    <%= f.label :middle_name %>:
    <%= f.text_field :middle_name %>
    <%= f.label :email, "E-Mail" %>:
    <%= f.text_field :email %>
     <%= f.label :mobile %>:
    <%= f.text_field :mobile %>
    <%= f.label :state %>:
    <%= f.text_field :state  %>
    <%= f.label :city %>:
    <%= f.text_field :city %>
    <%= f.label :zip_code %>:
    <%= f.text_field :zip_code %>
   <%= f.label :grade %>:
    <%= f.text_field :grade %>
    <%= f.label :enrollment_date %>:
    <%= f.text_field :enrollment_date %>
   
     <%= f.submit "Register" %>
  
<% end %> 

</div> 
<%= link_to 'Back', students_path %>



